<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android"
    android:interpolator="@anim/overshoot_interpolator"
    android:ordering="sequentially">

    <objectAnimator
        android:duration="1800"
        android:interpolator="@anim/overshoot_interpolator">
        <propertyValuesHolder
            android:propertyName="scaleX"
            android:valueFrom=".2f"
            android:valueTo=".5f"
            android:valueType="floatType" />

        <propertyValuesHolder
            android:propertyName="scaleY"
            android:valueFrom=".2f"
            android:valueTo=".5f"
            android:valueType="floatType" />

        <propertyValuesHolder
            android:propertyName="rotation"
            android:valueFrom="0f"
            android:valueTo="720f"
            android:valueType="floatType" />

    </objectAnimator>

    <objectAnimator
        android:duration="400"
        android:interpolator="@anim/overshoot_interpolator"
        android:propertyName="rotation"
        android:valueFrom="720f"
        android:valueTo="705f"
        android:valueType="floatType" />

    <objectAnimator
        android:duration="700"
        android:interpolator="@anim/overshoot_interpolator"
        android:propertyName="rotation"
        android:valueFrom="705f"
        android:valueTo="735f"
        android:valueType="floatType" />

    <objectAnimator
        android:duration="400"
        android:interpolator="@anim/overshoot_interpolator"
        android:propertyName="rotation"
        android:valueFrom="735f"
        android:valueTo="720f"
        android:valueType="floatType" />

    <objectAnimator
        android:duration="800"
        android:interpolator="@anim/overshoot_interpolator"
        android:propertyName="alpha"
        android:valueFrom="1f"
        android:valueTo="1f"
        android:valueType="floatType" />


    <objectAnimator
        android:duration="400"
        android:interpolator="@anim/overshoot_interpolator">

        <propertyValuesHolder
            android:propertyName="rotation"
            android:valueFrom="720f"
            android:valueTo="705f"
            android:valueType="floatType" />

        <propertyValuesHolder
            android:propertyName="scaleX"
            android:valueFrom="0.5f"
            android:valueTo="0.7f"
            android:valueType="floatType" />

        <propertyValuesHolder
            android:propertyName="scaleY"
            android:valueFrom="0.5f"
            android:valueTo="0.7f"
            android:valueType="floatType" />
    </objectAnimator>

    <objectAnimator
        android:duration="400"
        android:interpolator="@anim/overshoot_interpolator">

        <propertyValuesHolder
            android:propertyName="rotation"
            android:valueFrom="705f"
            android:valueTo="735f"
            android:valueType="floatType" />

    </objectAnimator>

    <objectAnimator
        android:duration="800"
        android:interpolator="@anim/overshoot_interpolator">

        <propertyValuesHolder
            android:propertyName="scaleY"
            android:valueFrom="0.7f"
            android:valueTo="0f"
            android:valueType="floatType" />

        <propertyValuesHolder
            android:propertyName="scaleX"
            android:valueFrom="0.7f"
            android:valueTo="0f"
            android:valueType="floatType" />

    </objectAnimator>


</set>